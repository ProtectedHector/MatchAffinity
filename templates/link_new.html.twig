{% extends 'common/base.html.twig' %}
{% import "macros/forms.html.twig" as forms %}

{% block body %}
    Available links: <br/>
    {% for link in links %}
        <i class="fas fa-external-link-alt mr-1"></i>{{ link.url }} <br/>
    {% endfor %}
    <hr>
    {{ form_start(form) }}

    {{ forms.form_field(form.url) }}
    {{ forms.form_field(form.type) }}
    {{ forms.form_field(form.submit) }}

    {{ form_end(form) }}
{% endblock %}

