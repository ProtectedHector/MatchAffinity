{% extends 'common/base.html.twig' %}

{% block body %}
    Available links: <br />
    {% for link in links %}
        <i class="fas fa-external-link-alt mr-1"></i>{{ link.url }} <br />
    {% endfor %}
    <hr>
    {{ form_start(form) }}
    <div class="form-group">
        {{ form_row(form.url) }}
    </div>
    <div class="form-group">
        {{ form_row(form.type) }}
    </div>
    <div class="form-group">
        {{ form_row(form.submit) }}
    </div>
    {{ form_end(form) }}
{% endblock %}

