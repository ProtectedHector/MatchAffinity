{% extends 'common/base.html.twig' %}
{% import "macros/forms.html.twig" as forms %}

{% block body %}
    {{ form_start(form) }}

    {{ forms.form_group(form.competition, path('new_competition')) }}
    {{ forms.form_group(form.season, path('new_season')) }}
    {{ forms.form_group(form.team1, path('new_team')) }}
    {{ forms.form_group(form.team2, path('new_team')) }}
    {{ forms.form_group(form.phase, path('new_phase')) }}

    {{ forms.form_field(form.funRate) }}
    {{ forms.form_field(form.historicRate) }}
    {{ forms.form_field(form.observations) }}
    {{ forms.form_field(form.dateLastSeen) }}
    {{ forms.form_field(form.times_seen) }}

    {{ forms.form_field(form.submit) }}

    {{ form_end(form) }}
{% endblock %}

